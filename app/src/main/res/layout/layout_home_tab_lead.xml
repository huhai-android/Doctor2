<?xml version="1.0" encoding="utf-8"?><!--头部菜单-->


    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/lv_tab_lead"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:layout_marginBottom="@dimen/dp_7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/lv_tab1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_marginTop="@dimen/dp_7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/im_picture"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26"
                        android:layout_centerHorizontal="true"
                        android:src="@mipmap/icon_yaoqinghuanzhe" />


                </RelativeLayout>

                <!--图标和消息图标-->


                <TextView
                    android:id="@+id/mFuncOne"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="邀请患者"
                    android:layout_marginBottom="@dimen/dp_7"
                    android:textColor="@color/plugin_camera_black" />
            </LinearLayout>

            <!--在线续方-->
            <LinearLayout
                android:id="@+id/lv_tab2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">


                <RelativeLayout
                    android:layout_marginTop="@dimen/dp_7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/im_renewal"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_zhuankexiezuo" />

                </RelativeLayout>


                <TextView
                    android:layout_marginBottom="@dimen/dp_7"
                    android:id="@+id/mFuncThree"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="专科协作"
                    android:textColor="@color/plugin_camera_black" />
            </LinearLayout>



            <!--胎心判读-->
            <LinearLayout
                android:id="@+id/lv_tab3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_marginTop="@dimen/dp_7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/invitate_patient_icon"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_youxuantuijian" />


                </RelativeLayout>

                <TextView
                    android:layout_marginBottom="@dimen/dp_7"
                    android:id="@+id/tv_chufang_check"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="优选推荐"
                    android:textColor="@color/plugin_camera_black" />
            </LinearLayout>

            <!--优选推荐-->
            <LinearLayout
                android:id="@+id/lv_tab4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_marginTop="@dimen/dp_7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/im_advice"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_wode_yaofang" />


                </RelativeLayout>

                <TextView
                    android:layout_marginBottom="@dimen/dp_7"
                    android:id="@+id/tv_youxuantuijian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="我的药房"
                    android:textColor="@color/plugin_camera_black" />
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_marginBottom="@dimen/dp_7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/lv_tab5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_marginTop="@dimen/dp_7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/performance_record_icon"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_mianhenyuyue" />


                </RelativeLayout>

                <TextView
                    android:layout_marginBottom="@dimen/dp_7"
                    android:id="@+id/tv_performance_record"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="面诊预约"
                    android:textColor="@color/plugin_camera_black" />
            </LinearLayout>

            <!--我的药房-->
            <LinearLayout
                android:layout_width="0dp"
                android:layout_marginTop="7dp"
                android:id="@+id/lv_tab6"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/dp_26"
                    android:layout_height="@dimen/dp_26"
                    android:src="@mipmap/icon_taixinpandu" />

                <TextView
                    android:layout_marginBottom="@dimen/dp_7"
                    android:id="@+id/mFuncFour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="胎心判读"
                    android:textColor="@color/plugin_camera_black" />
            </LinearLayout>


            <!--胎心判读-->

            <LinearLayout
                android:id="@+id/lv_tab7"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_marginTop="7dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/im_heart"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_fenjizhuanzhen" />


                </RelativeLayout>

                <TextView
                    android:layout_marginBottom="@dimen/dp_7"
                    android:id="@+id/tv_heartcheck"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="分级转诊"
                    android:textColor="@color/plugin_camera_black" />
            </LinearLayout>



            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:visibility="visible"></LinearLayout>


        </LinearLayout>





    </LinearLayout>





